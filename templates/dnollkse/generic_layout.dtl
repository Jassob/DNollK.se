<!DOCTYPE html>
<html lang="sv">
<head>

  <!-- Basic Page Needs
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <meta charset="utf-8">
  <title>
    {% block title %}{% endblock title%}
  </title>
  <meta name="description"
        content="{% block description %}                 
                 {% endblock description %}">
  <meta name="author" content="human.txt">

  <!-- Mobile Specific Metas
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- FONT
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  {% load staticfiles %}
  <link href="//fonts.googleapis.com/css?family=Raleway:400,300,600" rel="stylesheet" type="text/css">
  <link href="{% static "dnollkse/css/fonts.css" %}" rel="stylesheet">

  <!-- CSS
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <link rel="stylesheet" href="{% static "css/normalize.css" %}">
  <link rel="stylesheet" href="{% static "css/skeleton.css" %}">
  <link rel="stylesheet" href="{% static "css/shame.css" %}">
  {% block css %}
    <link rel="stylesheet" href="{% static "dnollkse/css/layout.css" %}">
  {% endblock css %}
  <!-- Favicon
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <link rel="icon" type="image/png" href="{% static "images/favicon.png" %}">

</head>
<body>

  <!-- Primary Page Layout
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <div class="container">
    <div class="row">

      <!-- Left sidebar, contains links to various social networks and what not
      ––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––-->
      <div class="three columns u-pull-left" id="sidebar">
        {% block sidebar %}
        {% endblock sidebar %}
      </div>

      <!-- Content container -->
      <div class="nine columns u-pull-right">
        
        <!-- header container
        ––––––––––––––––––––––––––––––––––––––––––––––––––-->
        <header class="u-full-width card header">
          {% block header %}
          {% endblock header %}
        </header> <!-- end header -->

        <!-- content container
        ––––––––––––––––––––––––––––––––––––––––––––––––––-->
        <section class="card content">
          {% block content %}
          {% endblock content %}
        </section> <!-- end content -->
        
        <!-- footer container
        ––––––––––––––––––––––––––––––––––––––––––––––––––-->
        <footer class="card">
          {% block footer %}
          {% endblock footer %}
        </footer>
      </div> <!-- end content container -->
    </div> <!-- end row -->
  </div> <!-- end root container -->
    
    <!-- End Document
    –––––––––––––––––––––––––––––––––––––––––––––––––– -->
</body>
</html>
