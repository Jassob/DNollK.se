{% extends "dnollkse/layout-with-sidebar.dtl" %}

{% block title %}FAQ - DNollK 2015{% endblock title %}

{% block content %}
    <div class="card center eight columns">
        <h1>FAQ</h1>
        {% if faq %}
            {% for f in faq %}
                <strong>{{ f.question|safe }}</strong>
                {{ f.answer|safe }}
            {% endfor %}
        {% else %}
            <p>Inga frågor har ställts!</p>
        {% endif %}
    </div>
{% endblock content %}